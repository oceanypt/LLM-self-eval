export type Obj = {\n        type: string;\n        [key: string]: any;\n    };\n    \n    export interface Person {\n        id: number;\n        name: string;\n        age: number;\n        address: string;\n    }\n    \n    export type Persons = Array<Obj>;\n    \n    export function getPersons(): Persons {\n        return [\n            {id: 1, name: 'John', age: 30, address: 'New York'},\n            {id: 2, name: 'Mary', age: 25, address: 'Los Angeles'},\n        ];\n    }\n    \n    export function getPersonById(id: number): Person | null {\n        const personId: number = id;\n        return getPersons().find((p: Obj) => p.id === personId);\n    }\n    \n    export function getPersonByName(name: string): Person | null {\n        const personName: string = 'John';\n        return getPersons().find((p: Obj) => p.name === personName);\n    }\n    \n    export function getPersonById(id: number): Person | null {\n        const personId: number = id;\n        return getPersons().find((p: Obj) => p.id === personId);\n    }\n    \n    export function getPersonByAddress(address: string): Person | null {\n        const personAddress: string = 'New York';\n        return getPersons().find((p: Obj) => p.address === personAddress);\n    }\n    \n    export function getPersonByNameAndAge(name: string, age: number): Person | null {\n        const personNameAndAge: string = 'John 30';\n        return getPersons().find((p: Obj) => p.name === personNameAndAge && p.age === age);\n    }\n    \n    export function getPersonByNameAndAddressAndAge(name: string, age: number, address: string): Person | null {\n        const personNameAndAge: string = 'John 30'+ address;\n        return getPersons().find((p: Obj) => p.name === personNameAndAge && p.age === age);\n    }\n    \n    export function getPersonByNameAndAgeAndAddress(name: string, age: number, address: string): Person | null {\n        const personNameAndAgeAndAddress: string = 'John 30'+ address +'30';\n        return getPersons().find((p: Obj) => p.name === personNameAndAgeAndAddress && p.age === age && p.address === address);\n    }
